<!-- views/admin.ejs -->
<h1>Admin Dashboard</h1>
<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Username</th>
        </tr>
    </thead>
    <tbody>
        <% data.forEach(user => { %>
            <tr>
                <td><%= user.id %></td>
                <td><%= user.username %></td>
            </tr>
        <% }); %>
    </tbody>
</table>
